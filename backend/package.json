  "description": "Desafio",
  "main": "index.js",
  "scripts": {
    "start": "JS=true node dist/index.js",
    "prestart": "JS=true npm run migrate",
    "dev": "nodemon src/index.ts",
    "predev": "npm run migrate",
    "migrate": "npm run typeorm migration:run",
    "revert": "npm run typeorm migration:revert",
    "create": "npm run typeorm migration:create -- -n",
    "generate": "npm run typeorm migration:generate -- -n",
    "build": "tsc --sourceMap false",
    "tsc": "tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "lint": "eslint . --ext .ts",
    "test": "rm -rf test; node --inspect=9998 node_modules/.bin/jest --watch"
  },
  "author": "Felipe Aguiar",
  "license": "UNLICENSED",
